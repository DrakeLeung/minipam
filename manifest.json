{
  "manifest_version": 2,

  "name": "minipam",
  "description": "a minimap for webpage",
  "version": "0.0.1",

  "content_scripts": [
    {
      "matches": [
        "http://*/**",
        "https://*/**"
      ],
      "js": ["minipam.js"],
      "css": ["app.css"],
      "run_at": "document_end"
    }
  ]
}
